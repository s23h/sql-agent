{
  "name": "sql-agent",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch --env-file=.env src/server/index.ts",
    "build": "pnpm run build:packages && pnpm run build:client && pnpm run build:server",
    "build:packages": "pnpm -r --filter './packages/*' run build",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/client/entry-server.tsx --outDir dist/server",
    "preview": "cross-env NODE_ENV=production tsx src/server/index.ts"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.23",
    "@claude-agent-kit/messages": "workspace:*",
    "@claude-agent-kit/server": "workspace:*",
    "@claude-agent-kit/websocket": "workspace:*",
    "@clerk/clerk-react": "^5.60.0",
    "@clerk/express": "^1.7.67",
    "@e2b/code-interpreter": "^2.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/vite": "^4.1.14",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "compression": "^1.8.1",
    "express": "^5.1.0",
    "jotai": "^2.15.0",
    "lucide-react": "^0.546.0",
    "nanoid": "^5.1.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^3.0.6",
    "react-syntax-highlighter": "^15.6.6",
    "remark-gfm": "^4.0.1",
    "sirv": "^3.0.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.14",
    "ws": "^8.18.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.3",
    "@types/node": "^24.5.1",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.0.4",
    "cross-env": "^10.0.0",
    "tsx": "^4.19.2",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.2",
    "vite": "^7.1.5"
  }
}
